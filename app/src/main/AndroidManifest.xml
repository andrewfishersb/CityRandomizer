<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fisher.hometown">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OldMainActivity"></activity>
        <activity android:name=".Zones.AfricaActivity" />
        <activity android:name=".Zones.AsiaActivity" />
        <activity android:name=".Zones.Europe.NorthernEurope" />
        <activity android:name=".Zones.Europe.SouthernEurope" />
        <activity android:name=".Zones.Europe.CentralEurope" />
        <activity android:name=".Zones.Europe.RestOfEurope" />
        <activity android:name=".Zones.Oceania" />
        <activity android:name=".Zones.America.NorthAmerica" />
        <activity android:name=".Zones.America.SouthAmerica" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BaseActivity"></activity>
    </application>

</manifest>